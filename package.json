{
  "name": "orbis-track",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "app",
    "server"
  ],
  "prisma": {
    "schema": "./server/src/infrastructure/database/prisma/schema.prisma"
  },
  "scripts": {
    "build": "npm run build -ws",
    "dev:app": "npm run dev -w app",
    "dev:server": "npm run dev -w server",
    "migrate:deploy": "prisma migrate deploy --schema ./server/src/infrastructure/database/prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema ./server/src/infrastructure/database/prisma/schema.prisma",
    "migrate:reset": "prisma migrate reset --schema ./server/src/infrastructure/database/prisma/schema.prisma",
    "generate": "prisma generate --schema ./server/src/infrastructure/database/prisma/schema.prisma",
    "db:seed": "tsx ./server/src/infrastructure/database/prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/404NotFoundTeam4/Orbis-Track.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/404NotFoundTeam4/Orbis-Track/issues"
  },
  "homepage": "https://github.com/404NotFoundTeam4/Orbis-Track#readme",
  "dependencies": {
    "@iconify/react": "^6.0.2",
    "@types/ioredis": "^4.28.10",
    "browser-image-compression": "^2.0.2",
    "ioredis": "^5.7.0"
  },
  "devDependencies": {
    "@iconify/tailwind": "^1.2.0",
    "@iconify/tailwind4": "^1.0.6",
    "@types/multer": "^2.0.0",
    "argon2": "^0.44.0",
    "axios": "^1.12.2",
    "ioredis": "^5.7.0",
    "multer": "^2.0.2"
  }
}